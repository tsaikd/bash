[user]
	name = tsaikd
	email = tsaikd@gmail.com
[alias]
	br = branch
	ci = commit
	cia = commit --amend
	co = checkout
	cotm = checkout -b master -t origin/master
	com = checkout origin/master
	di = diff
	sa = stash
	st = status
	ui = update-server-info
	llog = log --graph --decorate --all
	po = push origin
	pom = push origin master:refs/heads/master
	reba = rebase --abort
	rebc = rebase --continue
	sm = submodule
	smst = submodule status
	smup = submodule update --init --recursive
	sf = submodule foreach
	sfst = submodule foreach (git st || true)
[core]
	compression = 9
[color]
	branch = auto
	diff = auto
	status = auto
